              span.month
            .pricingContent
              ul
                li $12,740 Deposit
            .pricingTable-sign-up
              a.btn.btn-block(href='/types/house3' style='background-color:#70592a;') Details
  .team-boxed
  .register-photo(style='background-color:rgba(230, 227, 220, 0.34);')
    .form-container
      .image-holder(style='background-image:url("assets/img/houses.jpg");')
      form(method='post')
        h2.text-center(style="font-family:'Roboto Slab', serif;")
          strong Get in Contact
        .form-group
          input.form-control(type='email' name='email' required='' placeholder='Email')
        .form-group
          input.form-control(type='password' name='Telephone' required='' placeholder='Telephone')
        .form-group
          input.form-control(type='password' name='Message' required='' placeholder='Message')
        .form-group
          .form-check
            label.form-check-label
              input.form-check-input(type='checkbox')
              | I agree to the license terms.
        .form-group
          button.btn.btn-primary.btn-block(type='submit' style='background-color:#008b8b;') Email
        a.already(href='#')
  .footer-basic(style='background-color:#70592a;')
